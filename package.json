{
  "name": "whitecat-server",
  "version": "1.0.0",
  "description": "",
  "type": "module",
  "main": "src/index.js",
  "private": true,
  "scripts": {
    "acceptance": "jest --config acceptance/acceptance.config.json",
    "build": "tsc -p .",
    "build:dev": "nodemon --es-module-specifier-resolution=node --watch build build/index",
    "coverage": "jest --collectCoverage --config jest.config.json",
    "create-env": "printenv > .env",
    "dev": "yarn build && yarn build:dev",
    "docker:infra": "yarn docker:postgres",
    "docker:generate": "npx prisma generate",
    "docker:migrate": "npx prisma migrate deploy",
    "docker:migrate:dev": "npx prisma migrate dev",
    "docker:postgres": "cd infrastructure && docker-compose down && docker-compose up -d",
    "docker:postgres:stop": "cd infrastructure && docker-compose down",
    "docker:setup:data": "node --loader ts-node/esm --es-module-specifier-resolution=node infrastructure/data/index",
    "docker:setup": "yarn docker:generate && yarn docker:migrate:dev && yarn docker:setup:data",
    "docker:stop": "yarn docker:postgres:stop",
    "gcp-build": "tsc",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts' 'infrastructure/**/*.ts' --quiet",
    "prettier": "prettier --write .",
    "start": "node --loader ts-node/esm --es-module-specifier-resolution=node src/index",
    "unit": "jest --config jest.config.json",
    "verify": "yarn prettier && yarn lint && yarn coverage",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@graphql-tools/schema": "^7.1.3",
    "@prisma/client": "^2.23.0",
    "apollo-server": "^2.22.2",
    "date-fns": "^2.17.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "graphql": "^15.5.0",
    "ts-node": "^9.1.1"
  },
  "devDependencies": {
    "@actions/core": "^1.2.6",
    "@actions/github": "^4.0.0",
    "@babel/core": "^7.12.13",
    "@babel/node": "^7.12.13",
    "@babel/preset-env": "^7.13.15",
    "@babel/preset-typescript": "^7.13.0",
    "@graphql-codegen/cli": "1.21.5",
    "@graphql-codegen/typescript": "1.22.1",
    "@graphql-codegen/typescript-resolvers": "1.19.2",
    "@types/chance": "^1.1.1",
    "@types/jest": "^26.0.22",
    "@types/node": "^14.14.37",
    "@typescript-eslint/eslint-plugin": "^4.21.0",
    "@typescript-eslint/parser": "^4.21.0",
    "babel-jest": "^26.6.3",
    "chance": "^1.1.7",
    "concurrently": "^6.0.1",
    "eslint": "^7.19.0",
    "eslint-config-get-off-my-lawn": "^6.0.1",
    "eslint-config-prettier": "^7.2.0",
    "eslint-import-resolver-typescript": "^2.4.0",
    "eslint-plugin-prettier": "^3.3.1",
    "graphql-request": "^3.4.0",
    "jest": "^26.6.3",
    "jest-mock-extended": "^1.0.15",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "prisma": "^2.23.0",
    "ts-jest": "^26.5.4",
    "typescript": "^4.2.4"
  }
}
